@Test
public void si_se_alimenta_con_10_gramos_de_chocolate_y_tiene_5_de_energia_su_energia_final_es_95() {
  Golondrina pepita = new Golondrina();
  pepita.energia = 5;
  pepita.comerChocolate(10);
  Assert.assertEquals(95, pepita.energia);
}

@Test
public void si_se_alimenta_con_10_gramos_de_alpiste_y_tiene_5_de_energia_su_energia_final_es_25() {
  Golondrina pepita = new Golondrina();
  pepita.energia = 5;
  pepita.comerAlpiste(10);
  Assert.assertEquals(25, pepita.energia);
}

@Test
public void si_se_alimenta_con_10_gramos_de_sesamo_y_tiene_5_de_energia_su_energia_final_es_20() {
  Golondrina pepita = new Golondrina();
  pepita.energia = 5;
  pepita.comerSesamo(10);
  Assert.assertEquals(20, pepita.energia);
}